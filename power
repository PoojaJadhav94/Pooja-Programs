import java.util.Scanner;
class power
{
	public static void main(String args[])
	{
		int n,i,pow;
		
		Scanner sc=new Scanner(System.in);
		System.out.println("Enter the Nth number:");
		n=sc.nextInt();

		if(n<31)
		{
			for(i=1;i<n;i=i+2)
			{
				pow=2*i;
			}
			System.out.println("Power of 2 is: "+pow);
		}
	}
}
